import java.util.*;
public class FindTheMedian{
    public static void main(String args[]){
        Scanner scan=new Scanner(System.in);
        int n=scan.nextInt();
        int arr[]=new int[n];
        for(int i=0;i<n;i++){
            arr[i]=scan.nextInt();
        }
        Arrays.sort(arr);
        int medianposition=(arr.length/2);
        System.out.println(arr[medianposition]);
    }
}
